---
export interface Props {
  title: string;
  description: string;
  image: string;
  link: string;
}
const { title, description, image, link } = Astro.props;
---
<article class="project-card">
  <a href={link}>
    <img src={image} alt={title} />
    <h3>{title}</h3>
    <p>{description}</p>
  </a>
</article>

